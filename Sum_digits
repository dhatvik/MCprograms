#Sum of digits of a number 
 AREA SUMDIG,CODE,READONLY
DIVD RN 1
DIVS RN 2
Q RN 3
R RN 4
RES RN 5
ENTRY	LDR DIVD,=12345
		MOV RES,#0
		MOV DIVS,#10
LOOP BL DIV
		ADD RES,RES,R
		CMP Q,#0
		MOV DIVD,Q
		BNE LOOP
GO B GO
DIV MOV Q,#0
LOOP1 SUBS DIVD,DIVD,DIVS
	  ADDPL Q,Q,#1
	  BPL LOOP1
	  ADDMI R,DIVD,DIVS
	  BX LR
	  END
